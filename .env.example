APP_NAME=AAQ Backend
ENV=dev

DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=
DATABASE_PASSWORD=
DATABASE_NAME=postgres

N8N_BASE_URL=https://n8n.aishell.de
N8N_API_KEY=

# ============================================================================
# TradingAgents .env example (Google Gemini-first setup)
# ============================================================================

# LLM credentials
# For Gemini, set GOOGLE_API_KEY (or use Google ADC in your environment).
GOOGLE_API_KEY=

# Optional other providers (leave empty if not used)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
XAI_API_KEY=
OPENROUTER_API_KEY=

# Non-interactive run mode (run_env.py / trading_strategy.py)
# Required
TA_TICKER=Xiaomi
# Optional: defaults to today's date if empty
TA_ANALYSIS_DATE=

# Analyst switches
TA_ANALYST_MARKET=true
TA_ANALYST_SOCIAL=true
TA_ANALYST_NEWS=true
TA_ANALYST_FUNDAMENTALS=true

# Debate/risk depth (integer >= 1)
TA_RESEARCH_DEPTH=1

# Provider + models (Gemini 3 defaults)
TA_LLM_PROVIDER=google
TA_BACKEND_URL=https://generativelanguage.googleapis.com/v1
TA_QUICK_MODEL=gemini-3-flash-preview
TA_DEEP_MODEL=gemini-3-pro-preview

# Provider-specific thinking controls
# Gemini: high|minimal (leave empty to skip passing thinking mode)
TA_GOOGLE_THINKING_LEVEL=
# OpenAI only: low|medium|high
TA_OPENAI_REASONING_EFFORT=

# Runtime controls
TA_DEBUG=false
TA_SAVE_REPORT=true
# Optional output directory; if empty, defaults to ./results/<ticker>/<date>
TA_REPORT_DIR=
# Optional integration test output dir
TA_INTEGRATION_OUTPUT_DIR=
# Optional input for trading_strategy.py
SIGNAL_JSON_PATH=
